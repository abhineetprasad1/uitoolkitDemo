.tabbed-menu-container {
    height: 10%;
    flex-direction: row;
    justify-content: space-around;
    flex-grow: 0;
    width: 100%;
    flex-shrink: 1;
    top: auto;
    bottom: 0;
}

.tabbed-menu-button {
    flex-grow: 1;
    margin-left: 0;
}

.tabbed-menu-button:active {
    background-color: rgb(250, 214, 8);
}

